{
  "name": "cloudpay",
  "version": "1.0.0",
  "scripts": {
    "prepare": "husky install",
    "ng": "ng",
    "start": "ng serve",
    "build-prod": "ng build --configuration production --base-href /cloudpay/capp/",
    "build-dev": "ng build --configuration development --base-href /cloudpay/capp/",
    "watch": "ng build --watch --base-href /cloudpay/capp/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "update": "ng update --force @angular/cli @angular/core"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && ng lint && lint-staged"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.2.7",
    "@angular/common": "16.2.7",
    "@angular/compiler": "16.2.7",
    "@angular/core": "16.2.7",
    "@angular/forms": "16.2.7",
    "@angular/localize": "16.2.7",
    "@angular/platform-browser": "16.2.7",
    "@angular/platform-browser-dynamic": "16.2.7",
    "@angular/router": "16.2.7",
    "@ng-bootstrap/ng-bootstrap": "^15.1.1",
    "@ngrx/effects": "16.2.0",
    "@ngrx/store": "16.2.0",
    "@ngrx/store-devtools": "16.2.0",
    "angular-datatables": "^16.0.0",
    "bootstrap": "^5.3.2",
    "datatables.net": "^1.13.6",
    "datatables.net-dt": "^1.13.6",
    "jquery": "^3.7.1",
    "luxon": "^3.4.3",
    "ngx-toastr": "17.0.2",
    "process": "0.11.10",
    "rxjs": "7.5.6",
    "tslib": "2.4.0",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.2.4",
    "@angular-eslint/builder": "16.2.0",
    "@angular-eslint/eslint-plugin": "16.2.0",
    "@angular-eslint/eslint-plugin-template": "16.2.0",
    "@angular-eslint/schematics": "16.2.0",
    "@angular-eslint/template-parser": "16.2.0",
    "@angular/cli": "16.2.4",
    "@angular/compiler-cli": "16.2.7",
    "@angular/language-service": "16.2.7",
    "@types/datatables.net": "^1.10.25",
    "@types/jasmine": "~4.3.0",
    "@types/jasminewd2": "2.0.10",
    "@types/jquery": "^3.5.22",
    "@types/node": "18.7.14",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "admin-lte": "3.2.0",
    "eslint": "^8.39.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "pretty-quick": "3.1.3",
    "ts-node": "10.9.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.3.0",
    "typescript": "5.1.6"
  }
}
