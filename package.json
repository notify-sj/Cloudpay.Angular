{
  "name": "cloudpay",
  "version": "1.0.0",
  "scripts": {
    "prepare": "husky install",
    "ng": "ng",
    "start": "ng serve",
    "build-prod": "ng build --configuration production --base-href /cloudpay/capp/",
    "build-dev": "ng build --configuration development --base-href /cloudpay/capp/",
    "watch": "ng build --watch --configuration development --base-href /cloudpay/capp/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "update": "ng update --force @angular/cli @angular/core"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && ng lint && lint-staged"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.7",
    "@angular/common": "16.2.7",
    "@angular/compiler": "16.2.7",
    "@angular/core": "16.2.7",
    "@angular/forms": "16.2.7",
    "@angular/localize": "16.2.7",
    "@angular/platform-browser": "16.2.7",
    "@angular/platform-browser-dynamic": "16.2.7",
    "@angular/router": "16.2.7",
    "@ngrx/effects": "^16.2.0",
    "@ngrx/store": "16.2.0",
    "@profabric/angular-components": "0.1.7",
    "bootstrap": "4.6.1",
    "core-js": "3.25.0",
    "luxon": "3.0.3",
    "ngx-toastr": "17.0.2",
    "process": "0.11.10",
    "rxjs": "7.5.6",
    "summernote": "./summernote",
    "tslib": "2.4.0",
    "uuid": "8.3.2",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.2.4",
    "@angular-eslint/builder": "16.2.0",
    "@angular-eslint/eslint-plugin": "16.2.0",
    "@angular-eslint/eslint-plugin-template": "16.2.0",
    "@angular-eslint/schematics": "16.2.0",
    "@angular-eslint/template-parser": "16.2.0",
    "@angular/cli": "16.2.4",
    "@angular/compiler-cli": "16.2.7",
    "@angular/language-service": "16.2.7",
    "@types/jasmine": "~4.3.0",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "18.7.14",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "admin-lte": "3.2.0",
    "eslint": "^8.39.0",
    "fs-web": "1.0.1",
    "husky": "8.0.1",
    "jasmine-core": "~4.4.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "lint-staged": "13.0.3",
    "os-browserify": "0.3.0",
    "path-browserify": "1.0.1",
    "prettier": "2.7.1",
    "pretty-quick": "3.1.3",
    "protractor": "7.0.0",
    "ts-node": "10.9.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.3.0",
    "typescript": "5.1.6"
  }
}
